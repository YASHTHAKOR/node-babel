{
  "name": "nodebabel",
  "version": "1.0.0",
  "description": "nodeJS boiler plate with express,mongodb and swagger",
  "main": "server/index.js",
  "scripts": {
    "test": "npm run test",
    "start": "nodemon server/index.js --exec babel-node",
    "build": "babel server -d dist",
    "serve": "node dist/index.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
   "*.js": ["eslint --fix", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YASHTHAKOR/node-babel.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "expressjs"
  ],
  "author": "YASH THAKOR",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YASHTHAKOR/node-babel/issues"
  },
  "homepage": "https://github.com/YASHTHAKOR/node-babel#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3"
  }
